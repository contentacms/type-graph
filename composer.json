{
    "name": "drupal-http-api/type-graph",
    "description": "Type graph component for the Drupal type system.",
    "homepage": "https://github.com/drupal-http-api/type-graph",
    "license": "GPL-2",
    "authors": [
        {
            "name": "Sebastian Siemssen",
            "email": "fubhy@fubhy.com"
        }
    ],
    "require": {
        "php": "~5.5|~7",
        "clue/graph": "^0.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Component\\TypeGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Drupal\\Component\\TypeGraph\\Tests\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
